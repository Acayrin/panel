{
  "name": "private",
  "description": "A Web-based minecraft server",
  "logo": "https://github.com/Acayrin/private/raw/master/logo.jpg",
  "repository": "https://github.com/Acayrin/private",
  "env": {
    "MINECRAFT_EULA": {
      "description": "Do you accept the Minecraft EULA?",
      "value": "true",
      "required": true
    },
    "DROPBOX_API_TOKEN": {
      "description": "Your personal dropbox API token",
      "required": true
    }
  },
  "buildpacks": [
    {
      "url": "heroku/jvm"
    },
    {
      "url": "https://github.com/Acayrin/private"
    },
    {
      "url": "https://github.com/Acayrin/private-php"
    }
  ]
}
